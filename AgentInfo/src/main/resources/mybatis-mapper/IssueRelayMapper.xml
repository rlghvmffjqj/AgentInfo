<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
        
<mapper namespace="issueRelay">
	<resultMap type="com.secuve.agentInfo.vo.IssueRelay" id="issueRelay"></resultMap>
	
	<insert id="insertIssueRelay">
		INSERT INTO issueRelay(issueRelayUrl, issueKeyNum)
		VALUES(#{issueRelayUrl}, ${issueKeyNum});
	</insert>
	
	<select id="getIssueRelayUrlOne" resultMap="issueRelay" parameterType="string">
		SELECT * FROM issueRelay WHERE issueRelayUrl LIKE CONCAT('%',#{issueRelayRandomUrl});
	</select>
	
	
</mapper>